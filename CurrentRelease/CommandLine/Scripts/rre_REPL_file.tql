remoteExtendScriptTarget = getEnv("RRE_REMOTE_URL");
inputFile = getEnv("RRE_SCRIPT_FILE");
timeoutMilliseconds = 10000;

extendScript = readFile(inputFile);

sendData(
    extendScript,
    remoteExtendScriptTarget, 
    G.CALL_BACK_ID_EXTENDSCRIPT, 
    function(result) {
        stdOut(result + "\n");
    },
    timeoutMilliseconds
);

quit();