remoteTQLTarget = getEnv("RRT_REMOTE_URL");
inputFile = getEnv("RRT_SCRIPT_FILE");
timeoutMilliseconds = 10000;

tqlScript = readFile(inputFile);

sendData(
    tqlScript,
    remoteTQLTarget, 
    G.CALL_BACK_ID_TQL,  // This constant is define in the startupScript.tql
    function(result) {
        stdOut(result + "\n");
    },
    timeoutMilliseconds
);

quit();